{
    "compilerOptions": {
      "target": "ESNext",
      "module": "ESNext",
      "jsx": "react-jsx",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "moduleResolution": "node",
      "allowJs": false,
      "noEmit": true,
      "isolatedModules": true
    },
    "include": ["src/**/*.ts", "src/**/*.tsx"]
  }
  